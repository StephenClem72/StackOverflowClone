<h1>Welcome to SteveOverflow</h1>

<h1>Listing all the motha fuckin' questions</h1>

<%=link_to 'Ask Question', new_question_path %>
<table>
<% @questions.each do |question| %>
  <tr>
    <td>
      <button><%=link_to '^', upvote_question_path(question), method: :patch%></button><br>
      <button><%=link_to 'v', downvote_question_path(question), method: :patch%></button>
    </td>
    <td><%=question.votes%></td>
    <td><h4><%=link_to question.title, question_path(question)%></h4></td>
    <!-- <p><%=question.content%></p> -->
  </tr>
<%end%>
</table>

<p><%=@quote%></p>